{
  "name": "landing-page",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "dev:normal": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "if exist .next rmdir /s /q .next && if exist node_modules rmdir /s /q node_modules && if exist package-lock.json del package-lock.json",
    "clean:powershell": "Remove-Item -Recurse -Force .next, node_modules, package-lock.json -ErrorAction SilentlyContinue",
    "reinstall": "npm run clean && npm install",
    "update-deps": "npm update"
  },
  "dependencies": {
    "lucide-react": "^0.468.0",
    "next": "^15.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.6", 
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.1",
    "eslint-config-next": "^15.4.2",
    "postcss": "^8.5.1",
    "tailwindcss": "^3.4.17",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}